<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homepage_bg"
    tools:context=".cash_in">

    <EditText
        android:id="@+id/editTextPhone3"
        android:layout_width="249dp"
        android:layout_height="73dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="phone"
        android:text=" SET AMOUNT"
        android:textColor="@color/dirtywhite"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="313dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="146dp"
        android:layout_height="146dp"
        app:srcCompat="@drawable/nobg"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="145dp" />

    <Button
        android:id="@+id/confcash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/white"
        android:text="Confirm"
        android:textColor="#086B45"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="397dp" />

    <Button
        android:id="@+id/backk"
        android:layout_width="103dp"
        android:layout_height="48dp"
        android:text="Back"
        android:textColor="#086B45"
        android:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="397dp" />
</androidx.constraintlayout.widget.ConstraintLayout>

